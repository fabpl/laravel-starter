{
    "name": "fabpl/laravel-starter",
    "description": "Opinionated starter kit for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fabpl",
            "email": "planchettefabrice@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5.0",
        "laravel/pint": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "Fabpl\\Starter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fabpl\\Starter\\StarterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
